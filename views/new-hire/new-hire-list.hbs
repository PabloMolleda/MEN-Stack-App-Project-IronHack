<div class="container">
    <h1>Employment Agreements' list</h1>
    <hr>
    {{#each newHires}}
    <p>- Document Ref.({{id}})</p>

    <a href="/company/hr/employment-agreement/preview/{{id}}" class='btn btn-info btn-sm'>
        Preview
    </a>
    <a href="/company/hr/employment-agreement/delete?newHire_id={{id}}" class='btn btn-info btn-sm'>
